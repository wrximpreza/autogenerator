body
  padding: 0px;
  margin: 0px;
  border: 0px;
  font-size : 0px;

.block_1
  width: 300px;
  height: 50px;
  background: #9DA1AA;
  font-family: Helvetica, Arial, sans-serif;

.block_1.width320
  width : 320px;

.block_1 .icon
  padding-left: 3px;
  padding-top : 2px;
  padding-top : 2px;
  width : 63px;
  height : 50px;
  float : left;

.block_1 .icon img
  width : 46px;
  height : 46px;

.block_1 .title
  width : 102px;
  float : left;
  height : 33px;
  overflow: hidden;
  color : #2d2d2d;
  font-size : 14px;
  padding-top : 9px;
  font-weight : bold;
  padding-right : 10px;

.block_1.width320 .title
  width : 122px;
  line-height: 50px;
  padding-top : 0px;

.block_1 .buttons
  background : #c4c7cb;
  float : right;
  height : 50px;
  width : 122px;

.block_1 .buttons .button
  width : 113px;
  height : 33px;
  border-radius: 20px;
  background : #27ae60;
  margin: 0 auto;
  margin-top: 9px;

.block_1 .buttons .button .os
    width : 72px;
    float : left;
    font-size : 14px;
    color : #fff;
    line-height : 33px;
    text-align : center;

.block_1 .buttons .button .os_img
    text-align : center;
    background : #2dc66e;
    width : 41px;
    float : left;
    height : 33px;
    line-height: 33px;
    border-radius : 0px 20px 20px 0px;

.block_1 .buttons .button .os_img img
    vertical-align : middle;



